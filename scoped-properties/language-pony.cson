'.source.pony':
  'editor':
    'tabLength': 2
    'commentStart': '// '
    'foldEndPattern': '^\\s*"""\\s*$'
    'increaseIndentPattern': '^\\s*(type|interface|trait|primitive|class|actor)\\b|\\b(if|then|else|elseif|for|in|do|while|repeat|until|try|with|match)\\b((?!end).)*$|(\\{[^\\}]*|\\([^\\)]*|\\[[^\\]]*)$|=>'
    'decreaseIndentPattern': '^\\s*(then|else|elseif|until|end|\\|)'
